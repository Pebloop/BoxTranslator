{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "main": "src/backend/main.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "electron .",
    "dist": "npm run build && electron-builder",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src/frontend/theme",
    "smui-theme-dark": "smui-theme compile public/build/smui-dark.css -i src/frontend/theme/dark"
  },
  "devDependencies": {
    "@material/typography": "^14.0.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@smui/button": "^7.0.0-beta.0",
    "@smui/layout-grid": "^7.0.0-beta.0",
    "@smui/list": "^7.0.0-beta.0",
    "@smui/menu": "^7.0.0-beta.0",
    "@smui/textfield": "^7.0.0-beta.0",
    "@tsconfig/svelte": "^2.0.0",
    "electron": "^21.3.1",
    "electron-builder": "^23.6.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-polyfill-node": "^0.11.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "smui-theme": "^7.0.0-beta.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@mdi/js": "^7.0.96",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "sirv-cli": "^2.0.0",
    "svelte-material-ui": "^7.0.0-beta.0"
  }
}
